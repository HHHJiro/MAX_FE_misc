{{ if $data && $data.result && $data.result.rank }}
{{set rankData = $data.result.rank.reverse() }}
{{ /if }}
<div class="ranking">
  <ul class="rank--filter">
    {{ each rankData rank index }}
      <li {{ if index == 0 }} class="active" {{ /if }}>{{ rank.desc }}</li>
    {{ /each }}
  </ul>
  <div id="rank" class="ranking__box">
    
  </div>
  {{ include './footer.art'}}
</div>