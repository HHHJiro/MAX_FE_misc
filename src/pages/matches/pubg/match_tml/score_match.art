{{set match_list = $data}}
{{if match_list.length == 0 }}
  <li>当日总表</li>
{{else if !($data.length >= 0)}}
<li>积分总表</li>
{{else}}
  {{ each match_list match index}}
  {{ if index == 0 }}
    <li>
      当日总表
    </li>
    {{ else }}
    {{if match.rank_list && match.rank_list.length > 0}}
      <li>
       {{ match.desc }}
      </li>
    {{/if}}
    {{/if}}
  {{/each}}
{{/if}}